        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="HTML">
        <![CDATA[<BSButton Color="$Color$">$selected$</BSButton>$end$]]>
      </Code>
      <Declarations>

  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>BSButtonFull</Title>
      <Shortcut>bs-button-full</Shortcut>
      <Description>Adds a button to your razor page.</Description>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="HTML">
        <![CDATA[<BSButton Color="$Color$" ButtonType="$ButtonType$" Outline="$Outline$" Size="$Size$" IsBlock="$Block$">$selected$$end$</BSButton>]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>Color</ID>
          <Default>Color.None</Default>
          <ToolTip>Select the color of the button.</ToolTip>
        </Literal>
        <Literal>
          <ID>ButtonType</ID>
          <Default>ButtonType.Button</Default>
          <ToolTip>Select the type of button this will be.</ToolTip>
        </Literal>
        <Literal>
          <ID>Outline</ID>
          <Default>false</Default>
          <ToolTip>Set to true if this is an outline styled button.</ToolTip>
        </Literal>
        <Literal>
          <ID>Size</ID>
          <Default>Size.None</Default>
          <ToolTip>Select the size you want the button to be.</ToolTip>
        </Literal>
        <Literal>
          <ID>Block</ID>
          <Default>false</Default>
          <ToolTip>Set to true if you want this to be a block styled button.</ToolTip>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>BSButtonGroup</Title>
      <Shortcut>bs-button-group</Shortcut>
      <Description>Adds a button group to your razor page.</Description>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="HTML">
        <![CDATA[<BSButtonGroup Size="$Size$" IsToolbar="$Toolbar$" IsVertical="$Vertical$"><BSButton Color="$Color$">$selected$</BSButton></BSButtonGroup>$end$]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>Size</ID>
          <Default>Size.None</Default>
          <ToolTip>Select the size you want the button group to be.</ToolTip>
        </Literal>
        <Literal>
          <ID>Toolbar</ID>
          <Default>false</Default>
          <ToolTip>Set to true if you want this button group to be a toolbar.</ToolTip>
        </Literal>
        <Literal>
          <ID>Color</ID>
          <Default>Color.None</Default>
          <ToolTip>Select the color you want the first button to be.</ToolTip>
        </Literal>
        <Literal>
          <ID>Vertical</ID>
          <Default>false</Default>
          <ToolTip>Set to true if you want vertical orientation.</ToolTip>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>

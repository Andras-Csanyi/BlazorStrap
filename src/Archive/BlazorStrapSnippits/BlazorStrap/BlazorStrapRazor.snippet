  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>BSAlert</Title>
      <Shortcut>bs-alert</Shortcut>
      <Description>Adds a BSAlert tag to your razor page.</Description>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="HTML">
        <![CDATA[<BSAlert Color="$Color$">$selected$$end$</BSAlert>]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>Color</ID>
          <Default>Color.Primary</Default>
          <ToolTip>Select the color you want for the Alert.</ToolTip>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
